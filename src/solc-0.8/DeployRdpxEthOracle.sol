pragma solidity ^0.8.0;

import {IRdpxEthOracle} from "./IRdpxEthOracle.sol";

contract DeployRdpxEthOracle {
    function deploy(address _pair) external returns (address oracle) {
        bytes
            memory bytecode = hex"";

        assembly {
            oracle := create(0, add(bytecode, 32), mload(bytecode))
        }

        IRdpxEthOracle(oracle).initialize(_pair, msg.sender);
    }
}
